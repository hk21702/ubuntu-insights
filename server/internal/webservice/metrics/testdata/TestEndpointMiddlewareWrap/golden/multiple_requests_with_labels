http_endpoint_request_size_bytes: |
    # HELP http_endpoint_request_size_bytes Tracks the size of HTTP requests to the endpoint.
    # TYPE http_endpoint_request_size_bytes summary
    http_endpoint_request_size_bytes_sum{code="202",handler="Multiple Requests with Labels",method="get",path="/test-get"} 62
    http_endpoint_request_size_bytes_count{code="202",handler="Multiple Requests with Labels",method="get",path="/test-get"} 2
    http_endpoint_request_size_bytes_sum{code="202",handler="Multiple Requests with Labels",method="post",path="/test-post"} 33
    http_endpoint_request_size_bytes_count{code="202",handler="Multiple Requests with Labels",method="post",path="/test-post"} 1
    http_endpoint_request_size_bytes_sum{code="202",handler="Multiple Requests with Labels",method="put",path="/test-put"} 31
    http_endpoint_request_size_bytes_count{code="202",handler="Multiple Requests with Labels",method="put",path="/test-put"} 1
http_endpoint_requests_total: |
    # HELP http_endpoint_requests_total Tracks the number of HTTP requests to the endpoint.
    # TYPE http_endpoint_requests_total counter
    http_endpoint_requests_total{code="202",handler="Multiple Requests with Labels",method="get",path="/test-get"} 2
    http_endpoint_requests_total{code="202",handler="Multiple Requests with Labels",method="post",path="/test-post"} 1
    http_endpoint_requests_total{code="202",handler="Multiple Requests with Labels",method="put",path="/test-put"} 1
