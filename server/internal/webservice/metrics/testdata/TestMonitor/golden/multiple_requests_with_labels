http_request_size_bytes: |
    # HELP http_request_size_bytes Tracks the size of HTTP requests.
    # TYPE http_request_size_bytes summary
    http_request_size_bytes_sum{code="202",handler="Multiple Requests with Labels",method="get",path="/test-get"} 62
    http_request_size_bytes_count{code="202",handler="Multiple Requests with Labels",method="get",path="/test-get"} 2
    http_request_size_bytes_sum{code="202",handler="Multiple Requests with Labels",method="post",path="/test-post"} 33
    http_request_size_bytes_count{code="202",handler="Multiple Requests with Labels",method="post",path="/test-post"} 1
    http_request_size_bytes_sum{code="202",handler="Multiple Requests with Labels",method="put",path="/test-put"} 31
    http_request_size_bytes_count{code="202",handler="Multiple Requests with Labels",method="put",path="/test-put"} 1
http_requests_total: |
    # HELP http_requests_total Tracks the number of HTTP requests.
    # TYPE http_requests_total counter
    http_requests_total{code="202",handler="Multiple Requests with Labels",method="get",path="/test-get"} 2
    http_requests_total{code="202",handler="Multiple Requests with Labels",method="post",path="/test-post"} 1
    http_requests_total{code="202",handler="Multiple Requests with Labels",method="put",path="/test-put"} 1
